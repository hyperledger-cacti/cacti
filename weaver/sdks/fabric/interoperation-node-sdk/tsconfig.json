{
    "compilerOptions": {
        "outDir": "./build",
        "allowJs": true,
        "target": "es5",
        "esModuleInterop": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "skipLibCheck": true
    },
    "include": ["src/**/*", "index.js"],
    "exclude": ["node_modules", "protos", "scripts", "test", "protos-js", "build"]
}