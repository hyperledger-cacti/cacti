    "@types/express": "4.17.8",
    "@types/uuid": "8.3.0",
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "fabric-network": "2.2.20",
    "socket.io": "4.5.4",
    "uuid": "8.3.2",
    "web3": "1.6.1",
    "web3-core": "1.6.1"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
