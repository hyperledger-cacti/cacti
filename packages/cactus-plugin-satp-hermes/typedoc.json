{
  "$schema": "https://typedoc.org/schema.json",
  "name": "SATP Hermes Plugin API Documentation",
  "entryPoints": [
    "./src/main/typescript/plugin-satp-hermes-gateway.ts",
    "./src/main/typescript/plugin-satp-hermes-gateway-cli.ts",
    "./src/main/typescript/public-api.ts",
    "./src/main/typescript/api1",
    "./src/main/typescript/core",
    "./src/main/typescript/cross-chain-mechanisms",
    "./src/main/typescript/database",
    "./src/main/typescript/services",
    "./src/main/typescript/factory",
    "./src/main/typescript/types",
    "./src/main/typescript/utils"
  ],
  "entryPointStrategy": "resolve",
  "out": "./public/typedoc",
  "theme": "default",
  "includeVersion": true,
  "exclude": [
    "**/node_modules/**",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/dist/**",
    "**/build/**",
    "**/coverage/**",
    "**/fabric-contracts/**",
    "**/chaincode-typescript/**"
  ],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "readme": "./README.md",
  "excludeExternals": false,
  "validation": {
    "notExported": false,
    "invalidLink": true,
    "notDocumented": false
  },
  "requiredToBeDocumented": [],
  "treatValidationWarningsAsErrors": false,
  "categorizeByGroup": true,
  "groupOrder": [
    "Entry Points",
    "SATP Gateway",
    "Gateway CLI", 
    "API Layer",
    "Core - Protocol Engine",
    "Core - Session Management",
    "Core - Crash Recovery",
    "Core - Error Handling",
    "Core - Stage Handlers",
    "Core - Stage Services",
    "Cross-Chain Mechanisms",
    "Cross-Chain Bridge",
    "Cross-Chain Oracle",
    "Database & Persistence",
    "Database Repositories",
    "Services",
    "Network Identification",
    "Gateway Services",
    "Monitoring",
    "Validation",
    "Factory & Configuration",
    "Type Definitions",
    "Utilities",
    "*"
  ],
  "cleanOutputDir": true,
  "navigationLinks": {
    "Documentation": "./docs/",
    "SATP Protocol": "https://github.com/hyperledger-cacti/cacti/tree/main/packages/cactus-plugin-satp-hermes",
    "Hyperledger Cacti": "https://hyperledger-cacti.github.io/cacti/",
    "GitHub Repository": "https://github.com/hyperledger-cacti/cacti"
  },
  "searchInComments": true,
  "sort": ["source-order"],
  "treatWarningsAsErrors": false,
  "skipErrorChecking": true,
  
}
