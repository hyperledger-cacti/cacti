    "express": "4.17.3",
    "http-errors": "1.6.3",
    "js-yaml": "3.14.1",
    "jsonwebtoken": "9.0.0",
    "log4js": "6.4.1",
    "morgan": "1.10.0",
    "serve-favicon": "2.4.5",
    "shelljs": "0.8.5",
    "socket.io": "4.4.1",
    "web3": "1.8.1"
  },
  "devDependencies": {
    "@hyperledger/cactus-api-client": "1.1.3",
    "@hyperledger/cactus-common": "1.1.3",
