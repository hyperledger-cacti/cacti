    "@types/tar-stream": "2.2.1",
    "@types/temp": "0.9.1",
    "esm": "3.2.25",
    "temp": "0.9.4"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "publishConfig": {
    "access": "public"
  },
  "browserMinified": "dist/cactus-test-tooling.web.umd.min.js",
  "mainMinified": "dist/cactus-test-tooling.node.umd.min.js",
