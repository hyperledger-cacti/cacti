    "execa": "5.1.1",
    "fabric-ca-client": "2.2.10",
    "fabric-network": "2.2.10",
    "fs-extra": "10.0.0",
    "internal-ip": "6.2.0",
    "is-port-reachable": "3.0.0",
    "joi": "17.4.2",
    "js-yaml": "4.1.0",
    "keycloak-admin": "1.14.21",
    "lodash": "4.17.21",
    "node-ssh": "12.0.0",
    "node-forge": "1.3.0",
    "p-retry": "4.6.1",
    "run-time-error": "1.4.0",
    "tar-stream": "2.2.0",
    "temp": "0.9.4",
    "typescript-optional": "2.0.1",
    "web3": "1.5.2",
    "web3-core": "1.5.2",
    "uuid": "8.3.2"
