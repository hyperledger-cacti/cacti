    "fabric-ca-client": "2.2.10",
    "fabric-network": "2.2.10",
    "http-errors": "1.6.3",
    "jsonwebtoken": "8.5.1",
    "log4js": "6.4.0",
    "morgan": "1.9.1",
    "shelljs": "0.8.5",
    "socket.io": "4.1.3",
    "ts-node": "8.9.1",
    "web3": "1.7.0",
    "xmlhttprequest": "1.8.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-prettier": "4.0.0",
    "prettier": "2.5.1",
    "tslint": "6.0.0",
    "typescript": "3.9.10"
  }
