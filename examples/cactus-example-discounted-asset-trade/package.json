    "escape-html": "1.0.3",
    "ethereumjs-common": "1.5.2",
    "ethereumjs-tx": "2.1.2",
    "express": "4.16.4",
    "fabric-ca-client": "1.4.19",
    "fabric-client": "1.4.19",
    "fabric-network": "1.4.19",
    "http-errors": "1.6.3",
    "jsonwebtoken": "9.0.0",
    "log4js": "6.4.0",
    "morgan": "1.9.1",
    "shelljs": "0.8.5",
    "socket.io": "4.4.1",
    "ts-node": "8.9.1",
    "web3": "1.8.1",
    "xmlhttprequest": "1.8.0"
